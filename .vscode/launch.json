  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/server/bin/www",
      "env": { "NODE_ENV": "default" }
    },
    {
      "type": "node",
      "name": "Launch Dev",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "dev"],
      "env": { "NODE_ENV": "default" }
    }
  ]
}
